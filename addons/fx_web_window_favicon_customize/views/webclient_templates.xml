<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <template id="custom_web_layout" name="Custom Web Layout" inherit_id="web.layout">
        <xpath expr="//head/link[@rel='shortcut icon']" position="replace">
            <link type="image/x-icon" rel="shortcut icon" t-att-href="x_icon or '/fx_web_window_favicon_customize/static/src/img/favicon.ico'"/>
        </xpath>
    </template>
</odoo>